{
  "name": "selectise",
  "version": "1.1.0",
  "description": "A simple and light-weight custom select generator, built using Vanilla JS and no dependencies",
  "main": "dist/main/index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "format": "prettier-standard 'src/**/*.js'",
    "lint": "eslint src",
    "build": "webpack --config webpack.prod.js",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liranh85/selectise.git"
  },
  "keywords": [
